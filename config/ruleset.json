    {"id": "R6", "label": "ISSUER_BEGINNER", "condition": "cta_texts contain 'Подать заявку на листинг'", "description": "First-time issuer CTA"},
    {"id": "R7", "label": "INVESTOR_QUALIFIED", "condition": "term_scores.investor_qualified >= 3", "description": "Qualified investor terms"},
    {"id": "R8", "label": "INVESTOR_BEGINNER", "condition": "term_scores.investor_beginner >= 3", "description": "Educational investor content"},
